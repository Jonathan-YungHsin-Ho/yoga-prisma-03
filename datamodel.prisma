type User {
  id: ID! @id
  name: String!
}

type Posting {
	id: ID! @id
	coach: String!
	title: String!
	industry: Industry!
	location: String!
	price: Int!
	description: String!
  createdAt: DateTime! @createdAt
}

type Industry {
	id: ID! @id
	name: String! @unique
	postings: [Posting!]
}

type Tag {
	id: ID! @id
	name: String! @unique
	postings: [Posting!]
}